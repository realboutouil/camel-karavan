{
  "name": "karavan-space",
  "version": "4.14.2",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "cp-designer": "cp -r ../karavan-designer/src/designer src",
    "cp-knowledgebase": "cp -r ../karavan-designer/src/knowledgebase src",
    "cp-topology": "cp -r ../karavan-designer/src/topology src",
    "cp-public-metadata": "cp -r ../karavan-designer/public/metadata public",
    "copy": "npm run cp-designer && npm run cp-knowledgebase && npm run cp-topology && npm run cp-public-metadata",
    "dev": "npm run copy && vite --port 3003",
    "build": "npm run copy && tsc && vite build",
    "serve": "npm run copy && vite preview",
    "prod": "npm run copy && vite build && rsync -a build/* ../resources/META-INF/resources"
  },
  "proxy": "http://localhost:8080/",
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@patternfly/patternfly": "^6.4.0",
    "@patternfly/react-core": "^6.4.0",
    "@patternfly/react-table": "^6.4.0",
    "@patternfly/react-topology": "^6.4.0",
    "@types/js-yaml": "4.0.9",
    "@types/uuid": "11.0.0",
    "axios": "1.13.2",
    "html-to-image": "1.11.13",
    "karavan-core": "file:../karavan-core",
    "netlify-auth-providers": "^1.0.0-alpha5",
    "octokit": "^5.0.5",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-resizable-panels": "^3.0.6",
    "rxjs": "7.8.2",
    "usehooks-ts": "^3.1.1",
    "uuid": "13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@svgr/webpack": "^8.1.0",
    "@types/dagre": "^0.7.53",
    "@types/netlify-auth-providers": "^1.0.2",
    "@types/node": "24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.39.1",
    "monaco-editor": "^0.54.0",
    "typescript": "5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "overrides": {
    "@babel/runtime": "7.26.10"
  }
}
